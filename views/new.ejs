<% include partials/header %>

<div class="container">

  <h1>Create a new Campground</h1>
  <form action="/campgrounds" method="POST">
    <div class="form-group">
      <input class="form-control" id="nameInput" type="text" name="name" placeholder="name">
    </div>
    <div class="form-group">
      <input class="form-control" id="imgInput" type="text" name="image" placeholder="image url">
    </div>
    <div class="form-group">
      <button class="btn btn-lrg btn-primary btn-block">Submit</button>
      <a href="/campgrounds">Back</a>
    </div>
  </form>
</div>

<% include partials/footer %>